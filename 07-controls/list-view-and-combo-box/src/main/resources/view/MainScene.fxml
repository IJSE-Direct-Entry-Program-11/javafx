<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="363.0" prefWidth="592.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.dep11.MainSceneController">
   <children>
      <TextField fx:id="txtSubject" layoutX="14.0" layoutY="78.0" onAction="#txtSubjectOnAction" prefHeight="24.0" prefWidth="225.0" promptText="Eg. Algorithms">
         <font>
            <Font name="Ubuntu" size="14.0" />
         </font>
      </TextField>
      <ListView fx:id="lvSubjects" layoutX="14.0" layoutY="111.0" onKeyPressed="#lvSubjectsOnKeyPressed" prefHeight="200.0" prefWidth="225.0" style="-fx-font-family: Ubuntu; -fx-font-size: 14px;" />
      <ListView fx:id="lvSelectedSubjects" layoutX="352.0" layoutY="111.0" onKeyPressed="#lvSelectedSubjectsOnKeyPressed" prefHeight="200.0" prefWidth="225.0" style="-fx-font-family: Ubuntu; -fx-font-size: 14px;" />
      <Button fx:id="btnNext" layoutX="274.0" layoutY="156.0" mnemonicParsing="false" onAction="#btnNextOnAction" text="▶">
         <font>
            <Font name="Ubuntu" size="23.0" />
         </font>
      </Button>
      <Button fx:id="btnBack" layoutX="274.0" layoutY="214.0" mnemonicParsing="false" onAction="#btnBackOnAction" text="◀">
         <font>
            <Font name="Ubuntu" size="23.0" />
         </font>
      </Button>
      <Button fx:id="btnRemove" layoutX="14.0" layoutY="320.0" mnemonicParsing="false" onAction="#btnRemoveOnAction" prefHeight="24.0" prefWidth="106.0" text="Remove">
         <font>
            <Font name="Ubuntu" size="14.0" />
         </font>
      </Button>
      <Button fx:id="btnClear" layoutX="133.0" layoutY="320.0" mnemonicParsing="false" onAction="#btnClearOnAction" prefHeight="24.0" prefWidth="106.0" text="Clear">
         <font>
            <Font name="Ubuntu" size="14.0" />
         </font>
      </Button>
      <Label layoutX="14.0" layoutY="14.0" text="List View and Combo Box Demo">
         <font>
            <Font name="Ubuntu Bold" size="16.0" />
         </font>
      </Label>
      <Label layoutX="14.0" layoutY="54.0" text="Enter a subject to add">
         <font>
            <Font name="Ubuntu" size="14.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
